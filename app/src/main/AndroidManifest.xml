<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kelly.banner">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kelly.banner.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".recyclerbanner.RecyclerBannerActivity"></activity>
        <activity android:name=".recyclerbanner1.Recycler1BannerActivity"></activity>
        <activity android:name=".vpbanner1.VpBanner1Activity"></activity>
        <activity android:name=".vpbanner2.VpBanner2Activity"></activity>
        <activity android:name=".vpbanner3.VpBanner3Activity"></activity>
    </application>

</manifest>